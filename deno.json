{
  "tasks": {
    "dev": "deno run --allow-run main.ts",
    "test": "deno test --allow-run tests/*.test.ts",
    "compile-windows": "deno compile --ouput bin/mute-mic --target x86_64-pc-windows-msvc main.ts",
    "compile-mac": "deno compile --ouput bin/mute-mic --target x86_64-apple-darwin main.ts",
    "compile-linux": "deno compile --ouput bin/mute-mic --target x86_64-unknown-linux-gnu main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
